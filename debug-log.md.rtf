\uc0\u9474     large files. If omitted, reads the entire file (if feasible, up to a default limit).",                                          \u9474 \
\uc0\u9474                 "type": "number"                                                                                                    \u9474 \
\uc0\u9474               \}                                                                                                                     \u9474 \
\uc0\u9474             \},                                                                                                                      \u9474 \
\uc0\u9474             "required": [                                                                                                           \u9474 \
\uc0\u9474               "absolute_path"                                                                                                       \u9474 \
\uc0\u9474             ],                                                                                                                      \u9474 \
\uc0\u9474             "type": "object"                                                                                                        \u9474 \
\uc0\u9474           \}                                                                                                                         \u9474 \
\uc0\u9474         \}                                                                                                                           \u9474 \
\uc0\u9474       \},                                                                                                                            \u9474 \
\uc0\u9474       \{                                                                                                                             \u9474 \
