\uc0\u9474               \}                                                                                                                     \u9474 \
\uc0\u9474             \},                                                                                                                      \u9474 \
\uc0\u9474             "required": [                                                                                                           \u9474 \
\uc0\u9474               "file_path",                                                                                                          \u9474 \
\uc0\u9474               "content"                                                                                                             \u9474 \
