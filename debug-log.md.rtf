\uc0\u9474         "function": \{                                                                                                               \u9474 \
\uc0\u9474           "name": "search_file_content",                                                                                            \u9474 \
\uc0\u9474           "description": "Searches for a regular expression pattern within the content of files in a specified directory (or        \u9474 \
\uc0\u9474     current working directory). Can filter files by a glob pattern. Returns the lines containing matches, along with their file     \u9474 \
\uc0\u9474     paths and line numbers.",                                                                                                       \u9474 \
\uc0\u9474           "parameters": \{                                                                                                           \u9474 \
\uc0\u9474             "properties": \{                                                                                                         \u9474 \
\uc0\u9474               "pattern": \{                                                                                                          \u9474 \
\uc0\u9474                 "description": "The regular expression (regex) pattern to search for within file contents (e.g.,                    \u9474 \
\uc0\u9474     'function\\\\s+myFunction', 'import\\\\s+\\\\\{.*\\\\\}\\\\s+from\\\\s+.*').",                                                                \u9474 \
\uc0\u9474                 "type": "string"                                                                                                    \u9474 \
\uc0\u9474               \},                                                                                                                    \u9474 \
\uc0\u9474               "path": \{                                                                                                             \u9474 \
\uc0\u9474                 "description": "Optional: The absolute path to the directory to search within. If omitted, searches the current     \u9474 \
\uc0\u9474     working directory.",                                                                                                            \u9474 \
\uc0\u9474                 "type": "string"                                                                                                    \u9474 \
\uc0\u9474               \},                                                                                                                    \u9474 \
\uc0\u9474               "include": \{                                                                                                          \u9474 \
\uc0\u9474                 "description": "Optional: A glob pattern to filter which files are searched (e.g., '*.js', '*.\{ts,tsx\}', 'src/**'). \u9474 \
\uc0\u9474     If omitted, searches all files (respecting potential global ignores).",                                                         \u9474 \
\uc0\u9474                 "type": "string"                                                                                                    \u9474 \
\uc0\u9474               \}                                                                                                                     \u9474 \
\uc0\u9474             \},                                                                                                                      \u9474 \
\uc0\u9474             "required": [                                                                                                           \u9474 \
\uc0\u9474               "pattern"                                                                                                             \u9474 \
\uc0\u9474             ],                                                                                                                      \u9474 \
\uc0\u9474             "type": "object"                                                                                                        \u9474 \
\uc0\u9474           \}                                                                                                                         \u9474 \
\uc0\u9474         \}                                                                                                                           \u9474 \
\uc0\u9474       \},                                                                                                                            \u9474 \
\uc0\u9474       \{                                                                                                                             \u9474 \
\uc0\u9474         "type": "function",                                                                                                         \u9474 \
\uc0\u9474         "function": \{                                                                                                               \u9474 \
\uc0\u9474           "name": "glob",                                                                                                           \u9474 \
\uc0\u9474           "description": "Efficiently finds files matching specific glob patterns (e.g., `src/**/*.ts`, `**/*.md`), returning       \u9474 \
\uc0\u9474     absolute paths sorted by modification time (newest first). Ideal for quickly locating files based on their name or path         \u9474 \
\uc0\u9474     structure, especially in large codebases.",                                                                                     \u9474 \
\uc0\u9474           "parameters": \{                                                                                                           \u9474 \
\uc0\u9474             "properties": \{                                                                                                         \u9474 \
\uc0\u9474               "pattern": \{                                                                                                          \u9474 \
\uc0\u9474                 "description": "The glob pattern to match against (e.g., '**/*.py', 'docs/*.md').",                                 \u9474 \
\uc0\u9474                 "type": "string"                                                                                                    \u9474 \
\uc0\u9474               \},                                                                                                                    \u9474 \
\uc0\u9474               "path": \{                                                                                                             \u9474 \
\uc0\u9474                 "description": "Optional: The absolute path to the directory to search within. If omitted, searches the root        \u9474 \
\uc0\u9474     directory.",                                                                                                                    \u9474 \
\uc0\u9474                 "type": "string"                                                                                                    \u9474 \
\uc0\u9474               \},                                                                                                                    \u9474 \
\uc0\u9474               "case_sensitive": \{                                                                                                   \u9474 \
\uc0\u9474                 "description": "Optional: Whether the search should be case-sensitive. Defaults to false.",                         \u9474 \
\uc0\u9474                 "type": "boolean"                                                                                                   \u9474 \
\uc0\u9474               \},                                                                                                                    \u9474 \
\uc0\u9474               "respect_git_ignore": \{                                                                                               \u9474 \
\uc0\u9474                 "description": "Optional: Whether to respect .gitignore patterns when finding files. Only available in git          \u9474 \
\uc0\u9474     repositories. Defaults to true.",                                                                                               \u9474 \
\uc0\u9474                 "type": "boolean"                                                                                                   \u9474 \
\uc0\u9474               \}                                                                                                                     \u9474 \
\uc0\u9474             \},                                                                                                                      \u9474 \
\uc0\u9474             "required": [                                                                                                           \u9474 \
\uc0\u9474               "pattern"                                                                                                             \u9474 \
\uc0\u9474             ],                                                                                                                      \u9474 \
\uc0\u9474             "type": "object"                                                                                                        \u9474 \
\uc0\u9474           \}                                                                                                                         \u9474 \
\uc0\u9474         \}                                                                                                                           \u9474 \
\uc0\u9474       \},                                                                                                                            \u9474 \
\uc0\u9474       \{                                                                                                                             \u9474 \
\uc0\u9474         "type": "function",                                                                                                         \u9474 \
\uc0\u9474         "function": \{                                                                                                               \u9474 \
\uc0\u9474           "name": "replace",                                                                                                        \u9474 \
\uc0\u9474           "description": "Replaces text within a file. By default, replaces a single occurrence, but can replace multiple           \u9474 \
\uc0\u9474     occurrences when `expected_replacements` is specified. This tool requires providing significant context around the change to    \u9474 \
\uc0\u9474     ensure precise targeting. Always use the read_file tool to examine the file's current content before attempting a text          \u9474 \
\uc0\u9474     replacement.\\n\\n      The user has the ability to modify the `new_string` content. If modified, this will be stated in the      \u9474 \
\uc0\u9474     response.\\n\\nExpectation for required parameters:\\n1. `file_path` MUST be an absolute path; otherwise an error will be          \u9474 \
\uc0\u9474     thrown.\\n2. `old_string` MUST be the exact literal text to replace (including all whitespace, indentation, newlines, and        \u9474 \
\uc0\u9474     surrounding code etc.).\\n3. `new_string` MUST be the exact literal text to replace `old_string` with (also including all        \u9474 \
\uc0\u9474     whitespace, indentation, newlines, and surrounding code etc.). Ensure the resulting code is correct and idiomatic.\\n4. NEVER    \u9474 \
\uc0\u9474     escape `old_string` or `new_string`, that would break the exact literal text requirement.\\n**Important:** If ANY of the above   \u9474 \
\uc0\u9474     are not satisfied, the tool will fail. CRITICAL for `old_string`: Must uniquely identify the single instance to change. Include \u9474 \
\uc0\u9474     at least 3 lines of context BEFORE and AFTER the target text, matching whitespace and indentation precisely. If this string     \u9474 \
\uc0\u9474     matches multiple locations, or does not match exactly, the tool will fail.\\n**Multiple replacements:** Set                      \u9474 \
\uc0\u9474     `expected_replacements` to the number of occurrences you want to replace. The tool will replace ALL occurrences that match      \u9474 \
\uc0\u9474     `old_string` exactly. Ensure the number of replacements matches your expectation.",                                             \u9474 \
\uc0\u9474           "parameters": \{                                                                                                           \u9474 \
\uc0\u9474             "properties": \{                                                                                                         \u9474 \
\uc0\u9474               "file_path": \{                                                                                                        \u9474 \
\uc0\u9474                 "description": "The absolute path to the file to modify. Must start with '/'.",                                     \u9474 \
\uc0\u9474                 "type": "string"                                                                                                    \u9474 \
\uc0\u9474               \},                                                                                                                    \u9474 \
\uc0\u9474               "old_string": \{                                                                                                       \u9474 \
\uc0\u9474                 "description": "The exact literal text to replace, preferably unescaped. For single replacements (default), include \u9474 \
\uc0\u9474     at least 3 lines of context BEFORE and AFTER the target text, matching whitespace and indentation precisely. For multiple       \u9474 \
\uc0\u9474     replacements, specify expected_replacements parameter. If this string is not the exact literal text (i.e. you escaped it) or    \u9474 \
\uc0\u9474     does not match exactly, the tool will fail.",                                                                                   \u9474 \
\uc0\u9474                 "type": "string"                                                                                                    \u9474 \
\uc0\u9474               \},                                                                                                                    \u9474 \
\uc0\u9474               "new_string": \{                                                                                                       \u9474 \
\uc0\u9474                 "description": "The exact literal text to replace `old_string` with, preferably unescaped. Provide the EXACT text.  \u9474 \
\uc0\u9474     Ensure the resulting code is correct and idiomatic.",                                                                           \u9474 \
\uc0\u9474                 "type": "string"                                                                                                    \u9474 \
\uc0\u9474               \},                                                                                                                    \u9474 \
\uc0\u9474               "expected_replacements": \{                                                                                            \u9474 \
\uc0\u9474                 "type": "number",                                                                                                   \u9474 \
\uc0\u9474                 "description": "Number of replacements expected. Defaults to 1 if not specified. Use when you want to replace       \u9474 \
\uc0\u9474     multiple occurrences.",                                                                                                         \u9474 \
\uc0\u9474                 "minimum": 1                                                                                                        \u9474 \
\uc0\u9474               \}                                                                                                                     \u9474 \
\uc0\u9474             \},                                                                                                                      \u9474 \
\uc0\u9474             "required": [                                                                                                           \u9474 \
\uc0\u9474               "file_path",                                                                                                          \u9474 \
\uc0\u9474               "old_string",                                                                                                         \u9474 \
\uc0\u9474               "new_string"                                                                                                          \u9474 \
\uc0\u9474             ],                                                                                                                      \u9474 \
\uc0\u9474             "type": "object"                                                                                                        \u9474 \
\uc0\u9474           \}                                                                                                                         \u9474 \
\uc0\u9474         \}                                                                                                                           \u9474 \
\uc0\u9474       \},                                                                                                                            \u9474 \
\uc0\u9474       \{                                                                                                                             \u9474 \
\uc0\u9474         "type": "function",                                                                                                         \u9474 \
\uc0\u9474         "function": \{                                                                                                               \u9474 \
\uc0\u9474           "name": "write_file",                                                                                                     \u9474 \
\uc0\u9474           "description": "Writes content to a specified file in the local filesystem.\\n\\n      The user has the ability to modify   \u9474 \
\uc0\u9474     `content`. If modified, this will be stated in the response.",                                                                  \u9474 \
\uc0\u9474           "parameters": \{                                                                                                           \u9474 \
\uc0\u9474             "properties": \{                                                                                                         \u9474 \
\uc0\u9474               "file_path": \{                                                                                                        \u9474 \
\uc0\u9474                 "description": "The absolute path to the file to write to (e.g., '/home/user/project/file.txt'). Relative paths are \u9474 \
\uc0\u9474     not supported.",                                                                                                                \u9474 \
\uc0\u9474                 "type": "string"                                                                                                    \u9474 \
\uc0\u9474               \},                                                                                                                    \u9474 \
