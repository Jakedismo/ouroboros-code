\uc0\u9474         \}                                                                                                                           \u9474 \
\uc0\u9474       \},                                                                                                                            \u9474 \
\uc0\u9474       \{                                                                                                                             \u9474 \
\uc0\u9474         "type": "function",                                                                                                         \u9474 \
\uc0\u9474         "function": \{                                                                                                               \u9474 \
\uc0\u9474           "name": "web_fetch",                                                                                                      \u9474 \
\uc0\u9474           "description": "Processes content from URL(s), including local and private network addresses (e.g., localhost), embedded  \u9474 \
